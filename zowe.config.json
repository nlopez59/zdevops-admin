{
    "$schema": "./zowe.schema.json",
    
    "Notes": "A sample RSEAPI zowe profile. Change host, port and other values to match your env",

    "profiles": {
    
        
        "taz-zos-zosmf": {
            "type": "zosmf",
            "properties": {
                "host": "taz-zos",
                "port": 10443,
                "protocol": "https",
                "rejectUnauthorized": false
            },
            "secure": [
                "user",
                "password"
            ]
        },
    },

    "autoStore": true,

    "defaults": { 
        "zosmf": "taz-zos-zosmf"
        
    }
}